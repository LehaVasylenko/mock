y.jvar_high_contrast;\n\n\t\t\t  imgsrc;\n\t\t  </g:evaluate>\n\t      <!-- no image, provide default -->\n\t      <j:if test=\"$[jvar_choice.image == '']\">\n\t        <j:if test=\"$[jvar_choice.selected]\">\n\t          <j:set var=\"jvar_image\" value=\"icon-check-circle\"/>\n\t        </j:if>\n\t        <j:if test=\"$[!jvar_choice.selected]\">\n\t            <j:set var=\"jvar_image\" value=\"icon-empty-circle\"/>\n\t        </j:if>\n\t      </j:if>\n\n\t\t  <g:set_if var=\"jvar_title\" test=\"$[jvar_choice.getParameter('title') == undefined]\" true=\"$[jvar_choice.label]\" false=\"$[jvar_choice.getParameter('title')]\" />\n\n\t      <span id=\"stage.$[jvar_item_idx].$[jvar_expand_id].$[jvar_choice.value]\" stage=\"true\" selected=\"$[jvar_choice.selected]\" aria-expanded=\"false\">\n\n\t\t      \t  <li style=\"padding:2px; padding-bottom: 5px; display: $[jvar_display_type];\" tabindex=\"-1\" aria-label=\"$[jvar_title] ($[jvar_choice.label])\" title=\"$[jvar_title]\">\n\t\t      \t\t<g:set_if test=\"$[GlideMobileExtensions.runningTablet() || gs.getProperty('glide.ui.doctype') == 'true']\" var=\"jvar_display_type\"\n\t\t                \ttrue=\"inline-block\" false=\"inline\" />\n\t\t\t\t\t\t\t<img id=\"stage.$[jvar_item_idx].$[jvar_expand_id].$[jvar_choice.value].image\" title=\"$[jvar_title]\"\n\t\t                     class=\"nopadding\" src=\"images/heisenberg_icons/stage_icons/$[jvar_image].png\" alt=\"$[jvar_choice.label]\"/>\n\t\t\t\t\t  \t<div id=\"stage.$[jvar_item_idx].$[jvar_expand_id].$[jvar_choice.value].text\" style=\"display:none; padding-left: 6px; float: right\">\n\t\t\t\t\t  \t\t$[jvar_title] ($[jvar_choice.label])\n\t\t\t\t\t  \t\t<j:if test=\"$[jvar_show_approvers == true]\">\n\t\t\t\t\t\t\t\t<j:if test=\"$[size(jvar_choice.getParameter('approvers')) != 0]\">\n\t\t\t\t\t\t\t\t\t<p style=\"margin-bottom: 0px\">\n\t\t\t\t\t\t\t\t\t\t${gs.getMessage('Approvers')}\n\t\t\t\t\t\t\t\t\t\t<j:forEach var=\"jvar_approver\" items=\"$[jvar_choice.getParameter('approvers')]\">\n\t\t\t\t\t\t\t\t\t\t\t<span class=\"sr-only\">$[jvar_approver.label]</span>\n\t\t\t\t\t\t\t\t\t\t</j:forEach>\n\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t<ol style=\"list-style-type: none; padding-left: 20px\">\n\t\t\t\t\t\t\t\t\t\t<j:forEach var=\"jvar_approver\" items=\"$[jvar_choice.getParameter('approvers')]\">\n\t\t\t\t\t\t\t\t\t\t\t<li style=\"margin: 0px; padding: 2px\">$[jvar_approver.label]</li>\n\t\t\t\t\t\t\t\t\t\t</j:forEach>\n\t\t\t\t\t\t\t\t\t</ol>\n\t\t\t\t\t\t  \t\t</j:if>\n\t\t\t\t\t   \t\t</j:if>\n\t\t\t\t\t   </div>\n\t\t      \t  </li>\n\t\t  </span>\n\t    </j:if>\n\t  </j:forEach>\n\t  </ol>\n  </td>\n</tr>\n</table>\n\n<g:evaluate var=\"jvar_expanded_pref\" expression=\"gs.getPreference('workflow.expanded.$[jvar_ref_parent]')\" />\n\n<j:if test=\"$[jvar_expanded_pref]\">\n    <script>toggleItems('$[jvar_expand_id]');</script>\n</j:if>\n\t\n<script>\n\t// accessibility for toggle buttons\n\t$j('[id=\"toggle-button-$[jvar_expand_id]\"').on('click keypress', function() {\n\t\tif (this.children[0].src.indexOf('hide') > -1)\n\t\t\tthis.setAttribute('aria-expanded', false);\n\t\telse\n\t\t\tthis.setAttribute('aria-expanded', true);\n\t});\n</script>\t\n\n\n</j:jelly>","name":"Workflow-driven","options":"","sys_name":"Workflow-driven","sys_scope":{"link":"https://dev315171.service-now.com/api/now/v1/table/sys_scope/global","value":"global"},"category":"general","sys_created_by":"john.maher","table":"","sys_policy":""},{"display_value":"Progress Bar","scoped_name":"","column":"","sys_mod_count":"64","active":"true","description":"A simple progress bar renderer","sys_updated_on":"2019-01-31 16:18:42","sys_tags":"","sys_class_name":"column_renderer","sys_id":"d7fc6a321b201100adca1e094f071306","sys_package":{"link":"https://dev315171.service-now.com/api/now/v1/table/sys_package/41672bfb04b12210559073c06a02f7c6","value":"41672bfb04b12210559073c06a02f7c6"},"sys_update_name":"column_renderer_d7fc6a321b201100adca1e094f071306","sys_updated_by":"admin","media_type":"","sys_created_on":"2015-06-10 13:12:10","xml":"<?xml version=\"1.0\" encoding=\"utf-8\" ?>\n\n<j:jelly trim=\"true\" xmlns:j=\"jelly:core\" xmlns:g=\"glide\" xmlns:j2=\"null\" xmlns:g2=\"null\">\n\n<j:set var=\"jvar_renderer\" value=\"SimpleProgressBar\"/>\n\n<g:evaluate var=\"jvar_is_accessibility_enabled\" expression=\"gs.getPreference('glide.ui.accessibility', 'false')\" />\n\n<g:evaluate var=\"jvar_choices\" jelly=\"true\" object=\"true\">\n\n    // get the api\n    var api = new SNC.RendererAPI('$[jvar_renderer]');\n    var showValue = api.getOption('showValue')+'';\n    \n    // build the full set of choices including subflows\n    var choices = api.getParentWorkflowChoices(current, jelly.jvar_ref);\n   \n    choices;\n</g:evaluate>\n\n<g:evaluate var=\"jvar_increment\" jelly=\"true\" >\n    var increment = 100.00 / choices.getSize();\n    var percentComplete = 0;\n    var atEnd = true;\n\n    for (var i = 0; choices.getSize() > i; i++) {\n        percentComplete += increment;\n        if (choices.getChoice(i).getParameter('state')+'' === 'active') {\n            atEnd = false;\n            break;\n        }\n    }\n\n    percentComplete = atEnd ? 100 : parseInt(percentComplete);\n\n    increment;\n</g:evaluate>\n\n\n<j:set var=\"jvar_t\" value=\"$[jvar_ref]\" />\n\n<table id=\"workflow.progress\" border=\"1\" cellpadding=\"0\" cellspacing=\"0\" \n       style=\"width:100%; margin-top: 2px; background-color: transparent;\" >\n\n<j:if test=\"$[showValue]\">\n  <tr>\n      <td colspan=\"${choices.getSize()}\" style=\"width:100%; font-size:10px; border:none; text-align:center; background: #000066 url('progress_pctnotdone.gifx') repeat-x;\">\n          ${showValue==='true' ? percentComplete+'%' : ' '}\n      </td>\n  </tr>\n</j:if>\n\n  <tr id=\"progress\">\n    \n  <j:forEach var=\"jvar_choice\" items=\"$[jvar_choices]\">\n    <g:evaluate var=\"jvar_choice_visible\" jelly=\"true\">\n        var isVisible = true;\n        if (!api.getOption('showSkipped'))\n            if (jelly.jvar_choice.getParameter('state')+'' == 'skipped')\n                isVisible = false;\n\n        if (jelly.jvar_choice.value == '')\n            isVisible = false;\n\n        isVisible;\n    </g:evaluate>\n\n    <g:evaluate var=\"jvar_image\" jelly=\"true\">\n        var imgsrc = 'progress_pctnotdone.gifx';\n        if (jelly.jvar_choice.getParameter('state')+'' != 'pending')\n              imgsrc = 'progress_pctdone.gifx';\n        imgsrc;\n    </g:evaluate>\n\t  \n\t\n    <j:if test=\"$[jvar_choice_visible]\">\n\n\t  <j:if test=\"$[jvar_is_accessibility_enabled]\">\n\t\t  <td nowrap=\"true\" tabindex=\"0\" style=\"width:${jvar_increment}%; border-left:none; border-top:none; border-bottom:none; border-right: 1px solid white;  background: #000066 url('${imgsrc}') repeat-x;\">\n\t\t\t  $[SP] <span class=\"sr-only\">$[jvar_choice.getParameter('name')] - $[jvar_choice.getParameter('state')] </span>\n\t\t  </td>\n\t  </j:if>\n\t\t\n\t  <j:if test=\"$[!jvar_is_accessibility_enabled]\">\n\t\t  <td nowrap=\"true\" tabindex=\"-1\" style=\"width:${jvar_increment}%; border-left:none; border-top:none; border-bottom:none; border-right: 1px solid white;  background: #000066 url('${imgsrc}') repeat-x;\">\n\t\t\t\t $[SP]\n\t\t  </td>\n\t  </j:if>\n\t\n    </j:if>\n  </j:forEach>\n\n</tr>\n</table>\n\n\n</j:jelly>","name":"SimpleProgressBar","options":"{showValue:false}","sys_name":"Progress Bar","sys_scope":{"link":"https://dev315171.service-now.com/api/now/v1/table/sys_scope/global","value":"global"},"category":"general","sys_created_by":"john.maher","table":"","sys_policy":""}]}