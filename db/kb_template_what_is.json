roup":"","category":"","kb_category":{"link":"https://dev315171.service-now.com/api/now/v1/table/kb_category/728c8519db89030064dd36cb7c9619f4","value":"728c8519db89030064dd36cb7c9619f4"}},{"kb_introduction":"<div class=\"col-xs-10 col-md-9 col-lg-8 form-field input_controls\" style=\"box-sizing: border-box; min-height: 32px; padding-left: 15px; padding-right: 0px; float: left; width: 1328.11px; color: #343d47; font-family: SourceSansPro, &#39;Helvetica Neue&#39;, Arial; font-size: 13px;\">\n<p style=\"box-sizing: border-box; margin: 0px 0px 9px;\">Live Feed is a social IT application that provides a place to post and share content in a ServiceNow instance.</p>\n<p> </p>\n</div>","wiki":null,"rating":"","source":"","sys_updated_on":"2017-09-20 10:24:56","number":"KB0010002","sys_updated_by":"admin","latest_aqi":"","sys_created_on":"2017-09-20 10:21:19","sys_domain":{"link":"https://dev315171.service-now.com/api/now/v1/table/sys_user_group/global","value":"global"},"workflow_state":"published","text":"","sys_created_by":"admin","scheduled_publish_date":"","image":"","cmdb_ci":"","author":{"link":"https://dev315171.service-now.com/api/now/v1/table/sys_user/6816f79cc0a8016401c5a33be04be441","value":"6816f79cc0a8016401c5a33be04be441"},"can_read_user_criteria":"","active":"true","cannot_read_user_criteria":"","published":"2017-09-20","helpful_count":"","sys_domain_path":"/","version":{"link":"https://dev315171.service-now.com/api/now/v1/table/kb_version/6c8cbf3987110300b66b5cdac5cb0b64","value":"6c8cbf3987110300b66b5cdac5cb0b64"},"meta_description":"","kb_knowledge_base":{"link":"https://dev315171.service-now.com/api/now/v1/table/kb_knowledge_base/0aa3ffa7db7c030064dd36cb7c96197f","value":"0aa3ffa7db7c030064dd36cb7c96197f"},"meta":"","topic":"General","display_number":"KB0010002 v1.0","base_version":"","short_description":"Life Feed Application (demo data for a \"What is\" template)","roles":"","direct":"false","description":"","generated_with_now_assist":"false","disable_suggesting":"false","sys_class_name":"kb_template_what_is","article_id":"36c9fff587110300b66b5cdac5cb0b2b","sys_id":"36c9fff587110300b66b5cdac5cb0b2b","use_count":"0","flagged":"false","disable_commenting":"false","valid_to":"2100-01-01","retired":"","display_attachments":"false","latest":"true","summary":{"link":"https://dev315171.service-now.com/api/now/v1/table/kb_knowledge_summary/ababf73987110300b66b5cdac5cb0b98","value":"ababf73987110300b66b5cdac5cb0b98"},"sys_view_count":"1","revised_by":"","article_type":"text","sys_mod_count":"3","kb_explanation":"<p style=\"box-sizing: border-box; margin: 0px 0px 9px; color: #343d47; font-family: SourceSansPro, &#39;Helvetica Neue&#39;, Arial; font-size: 13px;\">Live Feed content forms a searchable knowledge source for sharing information within an organization. Main features include the following:</p>\n<ul id=\"c_GetStartedWithLiveFeed__ul_afg_2wm_2r\" style=\"box-sizing: border-box; padding: 0px; margin: 0px 0px 9px; color: #343d47; font-family: SourceSansPro, &#39;Helvetica Neue&#39;, Arial; font-size: 13px; list-style-position: inside;\"><li style=\"box-sizing: border-box;\">Users can post, reply to, and rate messages, including links and images.</li><li style=\"box-sizing: border-box;\">Administrators can set up automatic messages that are generated when specific records are updated throughout the system.</li><li style=\"box-sizing: border-box;\">Users can subscribe to receive email notifications when new messages are posted.</li></ul>\n<div style=\"box-sizing: border-box; color: #343d47; font-family: SourceSansPro, &#39;Helvetica Neue&#39;, Arial; font-size: 13px;\">Live feed includes different types of feeds.\n<figure id=\"c_GetStartedWithLiveFeed__fig_ipw_jwm_2r\" style=\"box-sizing: border-box; margin: 0px;\">My Live Feeds <img id=\"c_GetStartedWithLiveFeed__image_ks5_ywm_2r\" style=\"box-sizing: border-box; margin: 0px; padding: 0px; border: 0px; vertical-align: middle; outline-width: 0px; max-width: 100%; height: auto;\" src=\"https://docs.servicenow.com/bundle/jakarta-servicenow-platform/page/use/live-feed/image/MyLiveFeeds.png\" alt=\"My live feeds\" /></figure>\n<div style=\"box-sizing: border-box;\">Note: Live Feed v2 is active for all new instances by default. If you are upgrading from an earlier version of ServiceNow, you need to activate live feed v2 to use these features.</div>\n</div>","sys_tags":"","instrumentation_metadata":"","replacement_article":"","taxonomy_topic":"","view_as_allowed":"false","ownership_group":"","category":"","kb_category":{"link":"https://dev315171.service-now.com/api/now/v1/table/kb_category/769770dadb0d030064dd36cb7c96191c","value":"769770dadb0d030064dd36cb7c96191c"}}]}